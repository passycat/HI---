<view class="page">
    <view class="title-text">上传图片，会自动显示图片的主要特征标签</view>
    <view class="show">
        <image src="{{imgPath}}" mode="widthFix" bindtap="selectImg"></image>
    </view>
    <view class="lables">
        <view wx:for="{{lableList}}" class="lable-item" wx:key="this">
            {{item}}
        </view>
    </view>
    <view class='bartable'>
      <image src='/images/cloud_upload.png' style='width:27px;height:27px'></image>
      <text>上传</text>
    </view>

</view>